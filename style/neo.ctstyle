<CartoTypeStyleSheet name='OpenStreetMap Neo style' labelFormat=';ref+" "{font-style:italic}"("name:en")"' roadflags='0' background='#C4DFF6' border='lightgrey' landLayer='outline'>

    <defs>
        <macro id='standard-tunnel'>
            <scale max='75000'>
                <tunnel fade='0.5'/>
            </scale>
        </macro>

        <macro id='major-road-label'>
            <label font-size='75%,7pt' glow='white' glowWidth='7%,1' color='#777' case='upper' duplicate='1000m,512'/>
        </macro>

        <macro id='minor-road-bridge'>
            <bridge width='125%,1.2pt' border='#FCFBE7+0.5black' borderWidth='10%,1' endPath='h 128 l 128 128'/>
        </macro>

        <macro id='mid-road-label'>
            <label font-size='75%,6pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title' duplicate='1000m,256'/>
        </macro>

        <macro id='mid-road-bridge'>
            <bridge width='125%,1.2pt' border='#FCFBE7+0.5black' borderWidth='14%,1' endPath='h 128 l 128 128'/>
        </macro>

        <macro id='minor-road-label'>
            <label font-size='75%,6pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
        </macro>

        <macro id='island-label'>
            <label color='#444' glow='white' glowWidth='7%,1' case='assume-title' position='horizontal' maxPolygonLabelLength='4' wrapWidth='20em' font-size='240m,5pt,16pt' font-style='italic' priory='-4'/>
        </macro>

        <macro id='point-island-label'>
            <label maxScale='750000' color='#444' glow='white' glowWidth='7%,1' case='assume-title' position='point' maxPolygonLabelLength='4' wrapWidth='20em' font-size='240m,5pt,16pt' font-style='italic' priory='-4'/>
        </macro>

        <macro id='route-circle'>
            <icon canOverlap='true' width='36m@6000_180m@40000,10pt'>
                <svg width="110" height="110">
                    <circle fill="~color~" cx="55" cy="55" r="50" opacity="0.2"/>
                    <circle fill="~color~" cx="55" cy="55" r="10" opacity="0.5"/>
                    <circle fill="none" stroke="~color~" cx="55" cy="55" r="49" stroke-width="8"/>
                    <circle fill="none" stroke="white" cx="55" cy="55" r="53" stroke-width="1" opacity="0.5"/>
                </svg>
            </icon>
        </macro>
        <if exp='GL'>
            <macro id='route-highlight'>
                <highlight distance='6m@6000_30m@40000' fill='~color~' width='2m@6000_30m@40000,1.5pt'/>
                <highlight distance='-6m@6000_-30m@40000' fill='~color~' width='2m@6000_30m@40000,1.5pt'/>
                <line fill='none' width='8m@6000_50m@40000,1.5pt' truncationAtStart='20m@6000_100m@40000,6pt' truncationAtEnd='20m@6000_100m@40000,6pt'/>
                <repeatedSymbol fill='~color~' gap='4000' maxScale='50000' path='#broad-arrow' isLabel='yes'/>
            </macro>
        </if>
        <if exp='!GL'>
            <macro id='route-highlight'>
                <highlight distance='6m@6000_30m@40000' fill='~color~' width='2m@6000_30m@40000,1.5pt'/>
                <highlight distance='-6m@6000_-30m@40000' fill='~color~' width='2m@6000_30m@40000,1.5pt'/>
                <line fill='none' width='8m@6000_50m@40000,1.5pt' truncationAtStart='20m@6000_100m@40000,6pt' truncationAtEnd='20m@6000_100m@40000,6pt'/>
                <repeatedSymbol fill='~color~' gap='4000' maxScale='50000' path='#broad-arrow'/>
            </macro>
        </if>

        <macro id='traffic-highlight'>
            <condition exp='(Type bitand 0xFF) le ~maxspeed~'>
                <shape fill='~color~' opacity='0.3'/>
                <condition exp='(Type bitand 0x100) != 0'>
                    <highlight width='6m@6000_30m@75000,0.6pt' distance='6m@6000_30m@75000' fill='~color~' opacity='0.5'/>
                </condition>
                <condition exp='(Type bitand 0x200) != 0'>
                    <highlight width='6m@6000_30m@75000,0.6pt' distance='-6m@6000_-30m@75000' fill='~color~' opacity='0.5'/>
                </condition>
            </condition>
        </macro>

        <icon id='dot-icon' width='5m,1.2pt,6pt'>
            <svg width='100' height='100'>
                <circle fill='dimgrey' stroke='none' cx='50' cy='50' r='49'/>
            </svg>
        </icon>

        <icon id='peak-icon' width='10m,2.5pt,14pt'>
            <svg width='100' height='100'>
                <path fill='grey' stroke='none' d='M 0 100 L 100 100 L 50 0 z'/>
            </svg>
        </icon>

        <hachure angle='90' color='blue+0.9white' id='wetland-hachure' interval='2.5pt' width='0.75pt'/>

        <hachure angle='135' color='red' id='warning-hachure' interval='3pt' width='1pt'/>

        <!-- The 'water' and 'water-minor' macros make it easy for water features to be in the land layers, for backward compatibility, as well as the waterway layers. -->
        <macro id='water'>
            <condition exp='OsmType in { "wat","rvb","doc","rsv" }'>
                <shape fill='#C4DFF6'/>
                <label glow='#C4DFF6+0.3white' color='#C4DFF6+0.3black' duplicate='1000m,256'/>
            </condition>
        </macro>

        <macro id='water-minor'>
            <!-- WETLAND -->
            <condition exp='OsmType=="wet"'>
                <shape fill='url(#wetland-hachure)'/>
            </condition>
        </macro>

        <rampGradient id="pale-terrain-feet-height-ramp">
            <stop offset='100ft-height' color='darkseagreen+0.7white'/>
            <stop offset='600ft-height' color='palegreen+0.7white'/>
            <stop offset='1200ft-height' color='palegoldenrod+0.7white'/>
            <stop offset='3000ft-height' color='burlywood+0.7white'/>
            <stop offset='4500ft-height' color='lightsalmon+0.7white'/>
            <stop offset='6000ft-height' color='#BBBBBB+0.7white'/>
            <stop offset='9000ft-height' color='#CCCCCC+0.7white'/>
            <stop offset='12000ft-height' color='#EEEEEE+0.7white'/>
            <stop offset='18000ft-height' color='white'/>
            <stop offset='30000ft-height' color='white'/>
            <range start='0' end='1' color='#00000000' />
            <range start='1' end='100ft-height' color='darkseagreen+0.7white'/>
        </rampGradient>

        <def id='broad-arrow' string='M -450 -50 V 50 H 250 L -100 400 H 50 L 450 0 L 50 -400 H -100 L 250 -50 H -450 Z'/>
        <def id='arrow' string='M -350 25 V -25 H 0 V -100 L 250 0 L 0 100 V 25 Z'/>

        <macro id='one-way-arrow'>
            <zoom min='16'>
                <oneWayArrow path='#arrow' gap='2500' fill='#93A7AC' opacity='0.5' isLabel='yes'/>
            </zoom>
        </macro>

