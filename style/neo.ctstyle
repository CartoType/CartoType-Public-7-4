    </defs>

    <layer name='map-extent'>
        <shape fill='#C4DFF6'/>
    </layer>

    <layer name='outline'>
        <shape fill='#FCFBE7'/>
        <condition exp='@=2'> <!-- load only polygon objects - line objects sometimes occur and we don't want them -->
            <condition exp='OsmType=="isl"'>
                <macro ref='island-label'/>
            </condition>
        </condition>
    </layer>

    <layer name='image'>
        <shape fill='white'/>
    </layer>

    <zoom max='15'>
        <layer name='terrain-height-feet'>
            <shape fill='url(#pale-terrain-feet-height-ramp)'/>
        </layer>

        <layer name='terrain-shadow'>
            <shape fill='#AA9'/>
        </layer>
        <layer name='urban-area'>
            <shape fill='#FCFBE7+0.02black'/>
        </layer>
    </zoom>

    <!-- BOUNDARY LAYERS -->

    <scale min='1500000'>
        <layer name='boundary/major'>
            <condition exp='@=1'>
                <!-- lines -->
                <line fill='#324+0.7white' width='0.6pt' dashArray='5 1 1 1'/>
            </condition>
            <condition exp='@=2'>
                <!-- polygons -->
                <label color='#324+0.7white' glow='white' font-size='8pt' position='horizontal' wrapWidth='20em' case='assume-title' priority='-4'/>
            </condition>
        </layer>
    </scale>

    <scale min='750000' max='3000000'>
        <layer name='boundary/minor'>
            <condition exp='(Type bitand #FFFF)=4'>
                <condition exp='@=1'>
                    <!-- lines -->
                    <line fill='#324+0.7white' width='0.6pt' dashArray='2 1 1 1'/>
                </condition>
                <condition exp='@=2'>
                    <!-- polygons -->
                    <label color='#324+0.7white' font-size='6pt' position='horizontal' priority='-2' wrapWidth='20em' case='assume-title'/>
                </condition>
            </condition>
        </layer>
    </scale>

    <!-- PLACE LAYERS -->
    <zoom min='7' max='16'>
        <layer name='place/major'>
            <label color='#444' glow='white' glowWidth='7%,1' case='assume-title' priority='0' position='point' wrapWidth='20em'/>
            <zoom max='15'>
                <condition exp='OsmType=="cit"'>
                    <label priority='-4' font-size='240m,8pt,24pt'/>
                </condition>
            </zoom>
            <zoom min='9'>
                <condition exp='OsmType=="tow"'>
                    <label priority='-3' font-size='220m,7pt,22pt'/>
                </condition>
            </zoom>
            <condition exp='OsmType=="isl"'>
                <condition exp='@=0'>
                    <macro ref='point-island-label'/>
                </condition>
                <condition exp='@=2'>
                    <macro ref='island-label'/>
                </condition>
            </condition>
        </layer>
    </zoom>
    <zoom min='12'>
        <layer name='place/minor'>
            <label font-size='110m,5pt,16pt' color='#444' glow='white' glowWidth='7%,1' case='assume-title' priority='0' position='point' wrapWidth='20em'/>
            <condition exp='OsmType=="vil"'/>

            <scale max='100000'>
                <condition exp='OsmType=="sub"'/>
            </scale>

            <scale max='80000'>
                <condition exp='OsmType=="ham"'/>
            </scale>
        </layer>
    </zoom>

    <!-- LAND LAYERS -->
    <layer name='land/major'>
        <label glowWidth='7%,1' case='assume-title' color='#FCFBE7+0.4black' priority='0' position='horizontal' maxPolygonLabelLength='3' wrapWidth='10em' wrap='yes'/>
        <zoom min='12'>
            <!-- FOREST -->
            <condition exp='OsmType in { "for","woo" }'>
                <shape fill='#C3D9AD80'/>
                <label color='#C3D9AD+0.4black'/>
            </condition>
        </zoom>
        <zoom min='10'>
            <!-- NATURAL -->
            <condition exp='OsmType=="nat"'>
                <shape fill='#C3D9AD+0.25black' opacity='0.2' border='#C3D9AD+0.25black' borderWidth='1'/>
                <label color='#C3D9AD+0.4black'/>
            </condition>
        </zoom>
        <!-- BODY OF WATER -->
        <macro ref='water'/>
    </layer>
    <zoom min='12'>
        <layer name='land/minor'>
            <label glowWidth='7%,1' case='assume-title' color='#FCFBE7+0.4black' priority='0' position='horizontal' maxPolygonLabelLength='3' wrapWidth='10em' wrap='yes'/>
            <!-- COMMERCE -->
            <condition exp='OsmType in { "cmr","ind" }'>
                <shape fill='#FCFBE780+0.03black'/>
            </condition>
            <!-- CEMETERY -->
            <condition exp='OsmType=="cem"'>
                <shape fill='#D6DED2'/>
            </condition>
            <!-- SCHOOLS -->
            <condition exp='OsmType in { "col","sch","uni" }'>
                <shape fill='#FFF5CC'/>
                <label color='#FFF5CC+0.4black'/>
            </condition>
            <!-- SPORTS -->
            <condition exp='OsmType in { "gol","spp","spo","stm" }'>
                <shape fill='#B8E6B880'/>
                <label color='#B8E6B8+0.4black'/>
            </condition>
            <!-- GRASS -->
            <condition exp='OsmType=="gra"'>
                <shape fill='#E6F2C180'/>
            </condition>
            <!-- HOSPITAL -->
            <condition exp='OsmType=="hos"'>
                <shape fill='#E5C6C3'/>
                <label color='#E5C6C3+0.4black'/>
            </condition>
            <!-- PARKING -->
            <condition exp='OsmType=="pkg"'>
                <shape fill='#EEE'/>
            </condition>
            <!-- PARKS -->
            <condition exp='OsmType in { "par","com" }'>
                <shape fill='#DAF2C180'/>
                <label color='#DAF2C1+0.4black'/>
            </condition>
            <!-- SUMMITS: FELL, PEAK. VOLCANO -->
            <zoom min='13'>
                <condition exp='OsmType in { "fel","pea","vol" }'>
                    <label labelFormat=';+" "{font-style:italic}"("name:en")"+" "_ele' color='grey' wrapWidth='10em'/>
                    <icon ref='peak-icon'/>
                </condition>
            </zoom>
            <macro ref='water-minor'/>
        </layer>
    </zoom>

    <!-- WATERWAY LAYERS -->
        <layer name='waterway/major'>
            <label font-style='italic' color='#C4DFF6+0.3black' case='assume-title' glow='#C4DFF6+0.6white' glowWidth='7%,1' priority='1' position='centralpath' duplicate='1000m,256'/>
            <line fill='#C4DFF6'/>
            <shape fill='#C4DFF6'/>

            <macro ref='water'/>

            <zoom min='8'>
            <!-- RIVER -->
            <condition exp='OsmType=="riv"'>
                <line width='20m,0.6pt'/>
                <label font-size='75%,5pt' maxScale='400000'/>
            </condition>

            <!-- CANAL -->
            <condition exp='OsmType=="can"'>
                <line width='8m,0.6pt'/>
                <label font-size='75%,5pt' maxScale='400000'/>
            </condition>

            </zoom>
        </layer>
    <zoom min='14'>
        <layer name='waterway/minor'>

            <label font-style='italic' color='#C4DFF6+0.3black' case='assume-title' glow='#C4DFF6+0.6white' glowWidth='7%,1' priority='1' position='centralpath' duplicate='1000m,256'/>
            <line fill='#C4DFF6'/>
            <shape fill='#C4DFF6'/>

            <!-- STREAM -->
            <condition exp='OsmType=="str"'>
                <line width='8m,0.6pt'/>
                <label font-size='75%,5pt'/>
            </condition>

            <!-- DRAIN -->
            <condition exp='OsmType=="dra"'>
                <line width='4m,0.6pt'/>
                <label font-size='75%,5pt'/>
            </condition>

            <macro ref='water-minor'/>
        </layer>
    </zoom>

    <zoom min='16'>
        <layer name='building'>
            <zoom min='17'>
                <shape fill='#E4E0E0' height='yes'/>
                <label font-size='8m,3pt,6pt' glowWidth='7%,1' case='assume-title' color='#E4E0E0+0.2black' position='horizontal' maxPolygonLabelLength='3' wrapWidth='10em' labelFormat=';+" "{font-style:italic}"("name:en")"+" "addr:housenumber+" "addr:housename' wrap='yes'/>
            </zoom>
            <zoom max='17'>
                <shape fill='#E4E0E0' height='yes'/>
            </zoom>
        </layer>
    </zoom>

    <zoom min='17'>
        <layer name='address'>
            <label font-size='8m,3pt,6pt' glowWidth='7%,1' case='assume-title' color='#E4E0E0+0.2black' position='point' labelFormat=';+" "{font-style:italic}"("name:en")"+" "addr:housenumber+" "addr:housename'/>
        </layer>
    </zoom>

    <!-- START OF ROAD LAYER GROUP -->
    <layerGroup>

        <zoom max='8'>

            <layer name='road/major' road='true'>

                <!-- TRUNK -->
                <condition exp='RoadType=="trunk"'>
                    <line width='16m,0.5pt' fill='#E68A5C'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- MOTORWAY -->
                <condition exp='RoadType=="motorway"'>
                    <line width='32m,0.6pt' fill='#E65C5C'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

            </layer>

        </zoom>

        <zoom min='8' max='14'>

            <layer name='road/mid' road='true'>

                <!-- SECONDARY -->
                <condition exp='RoadType in { "secondary", "secondary link" }'>
                    <line width='16m,0.6pt' fill='#FFE873'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='mid-road-bridge'/>
                    <macro ref='mid-road-label'/>
                </condition>

            </layer>

            <layer name='road/major' road='true'>

                <!-- PRIMARY -->
                <condition exp='RoadType in { "primary", "primary link" }'>
                    <line width='12m@6000_72m@75000,0.3pt' fill='#FFC859'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- TRUNK LINK -->
                <condition exp='RoadType == "trunk link"'>
                    <line width='10m@6000_60m@75000,0.3pt' fill='#E68A5C'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- TRUNK -->
                <condition exp='RoadType == "trunk"'>
                    <line width='12m@6000_72m@75000,0.3pt' fill='#E68A5C'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- MOTORWAY LINK -->
                <condition exp='RoadType == "motorway link"'>
                    <line width='10m@6000_60m@75000,0.6pt' fill='#E65C5C'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- MOTORWAY -->
                <condition exp='RoadType == "motorway"'>
                    <line width='20m@6000_120m@75000,0.6pt' fill='#E65C5C'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

            </layer>

        </zoom>

        <zoom min='14'>

            <layer name='road/minor' road='true'>

                <!-- PEDESTRIAN -->
                <condition exp='RoadType == "pedestrian"'>
                    <line width='8m,0.3pt' fill='#FAFAF5' border='#FCFBE7+0.1black' borderWidth='10%'/>
                    <macro ref='minor-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='minor-road-bridge'/>
                </condition>

                <!-- SERVICE -->
                <condition exp='RoadType == "service"'>
                    <line width='12m,0.3pt' fill='white' border='#FCFBE7+0.1black' borderWidth='10%'/>
                    <macro ref='minor-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='minor-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- TRACK -->
                <condition exp='RoadType == "track"'>
                    <line width='4m,0.3pt' fill='white' border='#FCFBE7+0.1black' borderWidth='10%'/>
                    <macro ref='minor-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='minor-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- RESIDENTIAL -->
                <condition exp='RoadType == "residential"'>
                    <line width='12m,0.3pt' fill='white' border='#FCFBE7+0.4black' borderWidth='10%'/>
                    <macro ref='minor-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='minor-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- UNCLASSIFIED -->
                <condition exp='RoadType == "unclassified"'>
                    <line width='14m,0.3pt' fill='white' border='#FCFBE7+0.4black' borderWidth='10%'/>
                    <macro ref='minor-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='minor-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

            </layer>

            <layer name='road/mid' road='true'>

                <!-- TERTIARY -->
                <condition exp='RoadType in { "tertiary", "tertiary link" }'>
                    <line width='16m,0.3pt' fill='white' border='#FCFBE7+0.4black' borderWidth='10%'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='mid-road-label'/>
                    <macro ref='mid-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- SECONDARY -->
                <condition exp='RoadType in { "secondary", "secondary link" }'>
                    <line width='16m,0.6pt' fill='#FFE873+0.1white' border='#FFE873+0.1black' borderWidth='10%'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='mid-road-label'/>
                    <macro ref='mid-road-bridge'/>
                    <macro ref='one-way-arrow'/>
                </condition>

            </layer>

            <layer name='road/major' road='true'>

                <!-- PRIMARY -->
                <condition exp='RoadType in { "primary", "primary link" }'>
                    <line width='12m@6000_72m@75000,0.3pt' fill='#FFC859+0.1white' border='#FFC859+0.1black' borderWidth='10%'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- TRUNK LINK -->
                <condition exp='RoadType == "trunk link"'>
                    <line width='10m@6000_60m@75000,0.3pt' fill='#E68A5C+0.1white' border='#E68A5C+0.1black' borderWidth='10%'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- TRUNK -->
                <condition exp='RoadType == "trunk"'>
                    <line width='12m@6000_72m@75000,0.3pt' fill='#E68A5C+0.1white' border='#E68A5C+0.1black' borderWidth='10%'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                    <macro ref='one-way-arrow'/>
                </condition>

                <!-- MOTORWAY LINK -->
                <condition exp='RoadType == "motorway link"'>
                    <line width='10m@6000_60m@75000,0.6pt' fill='#E65C5C+0.1white' border='#E65C5C+0.1black' borderWidth='10%'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>

                <!-- MOTORWAY -->
                <condition exp='RoadType == "motorway"'>
                    <line width='20m@6000_120m@75000,0.6pt' fill='#E65C5C+0.1white' border='#E65C5C+0.1black' borderWidth='10%'/>
                    <macro ref='major-road-label'/>
                    <macro ref='standard-tunnel'/>
                </condition>


            </layer>

        </zoom>

    </layerGroup>

    <zoom min='14'>
        <layer name='path' roadflags='#1F001'>

            <!-- FOOTPATH -->
            <condition exp='OsmType in { "foo","wlk","hik" }'>
                <line width='1m,0.3pt' fill='lightgreen' dashArray='1 1'/>
                <label font-size='75%,5pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
            </condition>

            <!-- BRIDLEWAY -->
            <condition exp='OsmType=="bri"'>
                <line width='1.5m,0.5pt' fill='lightgreen' dashArray='2,1'/>
                <label font-size='75%,5pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
            </condition>

            <!-- SKI ROUTE -->
            <condition exp='OsmType=="ski"'>
                <line width='1.5m,0.5pt' fill='lightgreen' dashArray='3,2'/>
                <label font-size='75%,5pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
            </condition>

            <!-- STEPS -->
            <condition exp='OsmType=="ste"'>
                <line width='2m,1pt' fill='lightgreen' dashArray='0.5,0.5'/>
                <label font-size='75%,5pt' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
            </condition>

            <!-- CYCLEWAY -->
            <condition exp='OsmType=="cyc" or OsmType=="cyr"'>
                <line width='1.5m,0.5pt' fill='lightgreen' dashArray='5 3'/>
                <label font-size='75%,5pt' font-style='italic' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
                <oneWayArrow path='#arrow' gap='2000' fill='lightgreen' isLabel='yes' unitsPerLineWidth='80'/>
            </condition>

        </layer>

        <layer name='routerelation'>
            <label font-size='75%,5pt' font-style='italic' glow='white' glowWidth='7%,1' color='#777' case='assume-title'/>
        </layer>

    </zoom>

    <zoom min='10'>
        <layer name='railway' roadflags='#1F001'>

            <condition exp='OsmType in { "rly","pry" }'>
                <line width='8m,0.6pt' fill='#FCFBE7+0.1black'/>
            </condition>

            <condition exp='OsmType in { "nar","tra","lir","swy" }'>
                <line width='6m,0.6pt' fill='#FCFBE7+0.1black'/>
            </condition>

        </layer>
    </zoom>

    <!-- AMENITY LAYERS -->
    <zoom min='10'>
        <layer name='amenity/major' roadflags='#1F001'>
            <!-- DEFAULT -->
            <zoom min='14'>
                <label font-size='8m,6pt,16pt' color='darkgrey+black' glow='white' glowWidth='7%,1' case='assume-title' priority='-2' wrapWidth='10em'/>
            </zoom>

            <!-- AIRPORTS -->
            <condition exp='OsmType=="aer"'>
                <icon ref='airport-icon'/>
            </condition>

            <!-- AIRPORT RUNWAYS AND TAXIWAYS -->
            <condition exp='OsmType=="run"'>
                <line width='30m' fill='#ddc'/>
                <label maxScale='12000'/>
            </condition>
            <condition exp='OsmType=="twy"'>
                <line width='10m' fill='#ddc'/>
                <label maxScale='12000'/>
            </condition>

        </layer>
    </zoom>
    <zoom min='14'>
        <layer name='amenity/minor' roadflags='#1F001'>
            <!-- DEFAULT -->
            <label font-size='8m,6pt,16pt' color='darkgrey+black' glow='white' glowWidth='7%,1' case='assume-title' priority='-2' wrapWidth='10em'/>

            <!-- RAILWAY STATION -->
            <zoom min='14'>
                <condition exp='OsmType=="stn"'>
                    <icon ref='station-icon'/>
                    <label priority='-2' color='red+0.5orange+0.2black'/>
                    <scale max='75000'>
                        <shape fill='brown' opacity='0.1'/>
                    </scale>
                </condition>
            </zoom>

            <zoom min='16'>

                <!-- THEATRE -->
                <condition exp='OsmType=="the"'>
                    <icon ref='theatre-icon'/>
                </condition>

                <!-- CINEMA -->
                <condition exp='OsmType=="cin"'>
                    <icon ref='cinema-icon'/>
                </condition>

                <!-- CAR PARKING -->
                <condition exp='OsmType=="pkg"'>
                    <icon ref='parking-icon'/>
                </condition>

                <!-- TOILETS -->
                <condition exp='OsmType=="toi"'>
                    <icon ref='toilet-icon'/>
                </condition>
            </zoom>

            <zoom min='17'>
                <!-- RESTAURANT -->
                <condition exp='OsmType=="rst"'>
                    <icon ref='restaurant-icon'/>
                </condition>

                <!-- CAFE -->
                <condition exp='OsmType=="caf"'>
                    <icon ref='cafe-icon'/>
                </condition>

                <!-- PUB -->
                <condition exp='OsmType in { "pub","bar" }'>
                    <icon ref='pub-icon'/>
                </condition>

                <!-- ATM -->
                <condition exp='OsmType=="atm"'>
                    <icon ref='atm-icon'/>
                </condition>

                <!-- PHARMACY -->
                <condition exp='OsmType=="pha"'>
                    <icon ref='pharmacy-icon'/>
                </condition>

                <!-- TRAFFIC LIGHTS -->
                <condition exp='OsmType=="trf"'>
                    <label priority='-3'/>
                    <icon ref='traffic-light-icon'/>
                </condition>

            </zoom>

            <zoom min='14'>
                <!-- FUEL -->
                <condition exp='OsmType=="fue"'>
                    <icon ref='fuel-icon'/>
                </condition>
            </zoom>

            <!-- MOTORWAY JUNCTION -->
            <condition exp='OsmType=="jct"'>
                <scale max='15000'>
                    <label position='point' color='#E65C5C+0.1black' font-size='40m,4.5pt,8pt' labelFormat='_jr+{font-style:italic}"&#x000a;(";")"' wrapLines='0' wrapWidth='10em'/>
                </scale>
                <scale min='15000'>
                    <label position='point' color='#E65C5C+0.1black' font-size='40m,4.5pt,8pt' labelFormat='_jr'/>
                </scale>
            </condition>

            <zoom min='17'>
                <!-- OTHER AMENITIES -->
                <condition exp='OsmType in { "bnk","bst","bui","col","fis","gry","kin","pbu","plw","pof","pol","sch","swi","uni" }'>
                    <condition exp='@=0'>
                        <!-- The icon is displayed for point objects only (@=0 means 'object type = point'). -->
                        <label color='grey' font-size='7m,4.5pt'/>
                        <icon ref='dot-icon'/>
                    </condition>
                    <condition exp='@!=0'>
                        <label color='grey' font-size='7m,4.5pt' maxPolygonLabelLength='6'/>
                    </condition>
                </condition>
            </zoom>

        </layer>
    </zoom>

    <layer name='graticule'>
        <scale max='1500000'>
            <condition exp='Type lt 3600'>
                <line width='0.3pt' fill='#C4DFF6+0.1black'/>
                <label color='#C4DFF6+0.1black' font-size='5pt' baseline='text-after-edge'/>
            </condition>
        </scale>
        <condition exp='Type ge 3600'>
            <line width='0.5pt' fill='#C4DFF6+0.1black'/>
            <label color='#C4DFF6+0.1black' font-size='6pt' baseline='text-after-edge'/>
        </condition>
    </layer>

    <scale max='250000'>
        <layer name='nearby' roadflags='#F000'>
            <condition exp='_warning==0'>
                <shape border='ncsgreen' borderWidth='2m,0.6pt' fill='ncsgreen+#00000020'/>
                <line border='ncsgreen' borderWidth='2m,0.6pt' fill='ncsgreen+#00000020' width='30m,2pt'/>
                <label case='assume-title' color='ncsgreen' font-size='10m,5pt,8pt' glow='white' glowWidth='7%,0.5pt' priority='-21' wrapWidth='10em'/>
            </condition>
            <condition exp='_warning==1'>
                <shape border='red' borderWidth='2m,0.6pt' fill='url(#warning-hachure)'/>
                <line border='red' borderWidth='2m,0.6pt' fill='url(#warning-hachure)' width='30m,2pt'/>
                <label case='assume-title' color='red' font-size='10m,5pt,8pt' glow='white' glowWidth='7%,0.5pt' priority='-21' wrapWidth='10em'/>
            </condition>
        </layer>
    </scale>

    <!-- A layer for highlighted routes -->
    <layer name='route' roadflags='#F000'>
        <condition exp='!$ or $==0'>
            <macro color='orange' ref='route-highlight'/>
        </condition>
        <condition exp='$==1'>
            <macro color='ncsblue' ref='route-highlight'/>
        </condition>
        <condition exp='$ gt 1'>
            <macro color='ncsgreen' ref='route-highlight'/>
        </condition>
    </layer>

    <!-- Waypoints on routes. -->
    <layer name='waypoint' roadflags='#F000'>
        <label baseline='central-caps' color='grey+orange' font-size='20m@6000_90m@40000,7pt,12pt' labelFormat='{[padding:20%,0.6pt;background-color:#FFFFFFC0;border-color:orange+grey;border-width:12%,0.7;border-radius:50%,1.2pt};{]}' position='point' priority='-21' wrapLines='0' wrapWidth='30em'/>
    </layer>

    <!-- The vehicle position when routing -->
    <layer name='route-position' roadflags='#F000'>
        <macro color='ncsred' ref='route-circle'/>
        <label priority='-10'/>
    </layer>
    
    <!-- The vehicle heading when routing -->
    <layer name='route-vector' roadflags='#F000'>
        <line width='40m,12pt,42pt'/>
        <label priority='-9'/>
        <repeatedSymbol fill='blue' gap='0' isLabel='yes' maxScale='1500000' path='#arrow'/>
    </layer>

    <!-- The start of the route -->
    <layer name='route-start' roadflags='#F000'>
        <macro color='orange' ref='route-circle'/>
        <label priority='-9'/>
    </layer>

    <!-- The end of the route -->
    <layer name='route-end' roadflags='#F000'>
        <macro color='orange' ref='route-circle'/>
        <label priority='-9'/>
    </layer>

    <!-- A layer for found objects -->
    <layer name='found'>
        <shape border='ncsred+0.5white' borderWidth='2m,0.6pt' fill='#FFFF0080'/>
        <line border='ncsred+0.5white' borderWidth='2m,0.6pt' fill='#FFFF0080' width='30m,2pt'/>
    </layer>

    <!-- Pushpins -->
    <zoom min='12'>
        <layer name='pushpin'>
            <condition exp='@=0'>
                <!-- The icon is displayed for point objects only (@=0 means 'object type = point'). -->
                <icon width='20m,12pt,96pt' canOverlap='true' hotSpotY='100%'>
                    <svg width='30' height='80'>
                        <defs>
                            <linearGradient id="g0" x1="10" y1="0" x2="17" y2="0">
                                <stop offset="0%" stop-color="black"/>
                                <stop offset="40%" stop-color="white+0.2black"/>
                                <stop offset="100%" stop-color="black" />
                            </linearGradient>
                            <radialGradient id="g1" cx="40%" cy="40%" r="90%" fx="50%" fy="50%">
                                <stop offset="0%" stop-color="white"/>
                                <stop offset="100%" stop-color="teal" />
                            </radialGradient>
                        </defs>
                        <path stroke='url(#g0)' stroke-width='4' d='M 15 30 v 48'/>
                        <circle fill='url(#g1)' cx ='15' cy='15' r='15'/>
                    </svg>
                </icon>
                <label font-size='12m,6pt' color='teal' glow='white' glowWidth='7%,1' case='assume-title' wrapWidth='5em' priority='-20'/>
                <shape border='blue' fill='teal' opacity='0.5' borderWidth='2m,0.6pt'/>
                <line width='5m,0.6pt' fill='teal' opacity='0.5'/>
            </condition>
            <condition exp='@!=0'>
                <label font-size='12m,6pt' color='teal' glow='white' glowWidth='7%,1' case='assume-title' wrapWidth='5em' priority='-20'/>
                <shape border='blue' fill='teal' opacity='0.5' borderWidth='2m,0.6pt'/>
                <line width='5m,0.6pt' fill='teal' opacity='0.5'/>
            </condition>
        </layer>
    </zoom>

    <layer name='gpx' roadflags='#F000'>
        <line width='2m,1pt' fill='ncsred'/>
        <label color='ncsred' font-size='6pt' baseline='text-after-edge' case='title'/>
    </layer>

    <stratum/>

    <layer name='range'>
        <shape fill='ncsyellow' opacity='0.3'/>
    </layer>

    <!--
    Traffic information: speeds and prohibited roads and areas.
    The speed in kph is in bits 0...7; 0 means prohibited.
    Bit 8 is on if the left side of a line is to be highlighted.
    Bit 9 is on if the right side of a line is to be highlighted.
    -->
    <layer name='traffic'>
        <macro ref='traffic-highlight' maxspeed='10' color='red'/>
        <macro ref='traffic-highlight' maxspeed='30' color='orange'/>
        <macro ref='traffic-highlight' maxspeed='255' color='green'/>
    </layer>

    <labelLayer/>

</CartoTypeStyleSheet>
